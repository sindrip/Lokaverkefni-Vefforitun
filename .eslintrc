{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
      "no-var": ["error"],
      "no-use-before-define": 0,
      "linebreak-style":0,
      "no-undef": 0,
      "no-unused-labels": 0,
      "no-useless-concat": 0,
      "prefer-template": 0,
      "no-restricted-syntax": 0,
      "no-unused-vars": 0,
      "prefer-const": 0,
      "no-loop-func": 0,
      "no-param-reassign": 0,
      "guard-for-in": 0,
      "max-len": 0,
      "no-script-url": 0,
      "new-cap": 0,
    },
  "env": {
    "browser": true,
  },
  "globals": {},
  "plugins": []
}
